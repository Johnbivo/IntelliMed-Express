<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.inteliMedExpress.controllers.LoginController" spacing="10" alignment="CENTER">
    <padding>
        <Insets top="20" right="20" bottom="20" left="20" />
    </padding>

    <Label text="Simple FXML Application" style="-fx-font-size: 18px; -fx-font-weight: bold;" />

    <TextField fx:id="nameTextField" promptText="Enter your name" />

    <Button fx:id="submitButton" text="Submit" onAction="#handleSubmit" />

    <Label fx:id="resultLabel" />
</VBox>